    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch", //se fosse "launch" ele vai rodar a aplicacao. No caso de usar o attach, eu precis startar a aplicacao por fora e depois "attachar" o debug nela
            "protocol": "inspector",
            "restart": true,
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}
